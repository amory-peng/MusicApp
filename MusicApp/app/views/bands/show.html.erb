<h1> <%= @band.name %></h1>
<%= link_to "edit band", edit_band_url(@band) %>
<%= button_to "destroy band", band_url(@band), method: :delete %>

<h2>Albums</h2>
<ul>
  <% @band.albums.each do |album| %>
    <a href="<%= album_url(album)  %>"><li> <%= album.name %> </li></a>
  <% end %>
</ul>

<%= link_to "add album", new_band_album_url(@band) %>
