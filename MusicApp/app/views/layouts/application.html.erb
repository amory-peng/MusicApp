<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <h1>WELCOME <%= current_user.email %></h1>
      <%= button_to "logout", session_url(session), method: :delete %>
      <%= link_to "go to bands", bands_url %>
    <% end %>
    <%= yield %>
  </body>
</html>
